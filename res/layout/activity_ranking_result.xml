<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.unb.deolhonoenade.view.RankingResult"
    tools:ignore="MergeRootFrame" >

    <TextView
        android:id="@+id/textInstituicao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="30dp"
        android:layout_y="0dp"
        android:text="@string/text_Instituicao"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:contentDescription="Instituição" />

    <TextView
        android:id="@+id/textNota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="238dp"
        android:layout_y="0dp"
        android:text="@string/text_Nota"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:contentDescription="Nota" />

    <ListView
        android:id="@+id/listResult"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_x="0dp"
        android:layout_y="22dp" >
    </ListView>

</AbsoluteLayout>
